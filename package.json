    "@typescript-eslint/parser": "^5.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.0.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "date-fns": "^2.25.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1"
